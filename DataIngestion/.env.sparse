# Sparse Pipeline Environment Configuration
# Copy this to .env for sparse pipeline deployment

# Database Configuration
DB_HOST=db
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=postgres
DATABASE_URL=postgresql://postgres:postgres@db:5432/postgres

# Sparse Pipeline Configuration
SPARSE_MODE=true
SPARSE_START_DATE=2014-01-01
SPARSE_END_DATE=2014-12-31
SPARSE_BATCH_SIZE=24
SPARSE_MIN_ERA_ROWS=10
SPARSE_MIN_HOURLY_COVERAGE=0.1
SPARSE_MAX_INTERPOLATION_GAP=2
SPARSE_WINDOW_HOURS=24
SPARSE_SLIDE_HOURS=6
SPARSE_FEATURES_TABLE=sparse_features

# GPU Configuration
DISABLE_GPU=false
CUDA_VISIBLE_DEVICES=0
NVIDIA_VISIBLE_DEVICES=all
NVIDIA_DRIVER_CAPABILITIES=compute,utility
GPU_BATCH_SIZE=500

# Rust Pipeline Configuration
RUST_LOG=info
RUST_BACKTRACE=1
DATA_SOURCE_PATH=/app/data

# Model Builder Configuration
USE_SPARSE_FEATURES=true
FEATURE_TABLES=sparse_features
MLFLOW_TRACKING_URI=
GPU_TAG=default_gpu
EPOCHS=50
BATCH_SIZE=32
LEARNING_RATE=0.001

# MOEA Configuration
MOEA_DEVICE=cuda
MOEA_CONFIG_PATH=/app/config/moea_config_gpu.toml
MOEA_POPULATION_SIZE=100
MOEA_GENERATIONS=50
MOEA_N_REFERENCE_POINTS=92

# Python Environment
PYTHONUNBUFFERED=1
PYTHONPATH=/app

# Cache Busting
CACHEBUST=1